{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build/client",
  "devCommand": "npm run dev",
  "installCommand": "npm install && npx prisma generate",
  "functions": {
    "build/server/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/server/index.js"
    }
  ]
}